<patch-1.0>
   <comment type="patch/comment" sha="sha" name="Shows how a script can modify parameters" x="14" y="14"/>
   <obj type="script/script2" sha="1478c410b77725959c034de4a2456dd1198e9080" name="script2_1" x="14" y="42">
      <params/>
      <attribs>
         <text attributeName="script">
            <sText><![CDATA[char i;

void setup(void){
	i=0;
}

void loop(void){
	chThdSleepMilliseconds(500);
	
	// count with i...
	i++;
	if (i == 64) {
		i = 0;
	}

	// set parameter
	PExParameterChange(&parent2->PExch[PARAM_INDEX_c1_value],ConvertIntToFrac(i),0xFFFD);

	// set parameter to reverse count
	PExParameterChange(&parent2->PExch[PARAM_INDEX_c2_value],ConvertIntToFrac(63-i),0xFFFD);

	// set one bit to true
	PExParameterChange(&parent2->PExch[PARAM_INDEX_cb16__1_b16],(1<<(i&0x0F)),0xFFFD);

	// shows the bits of the counter
	PExParameterChange(&parent2->PExch[PARAM_INDEX_cb16__2_b16],i,0xFFFD);
}]]></sText>
         </text>
      </attribs>
   </obj>
   <obj type="ctrl/dial p" sha="1f21216639bb798a4ea7902940999a5bcfd0de90" name="c1" x="14" y="140">
