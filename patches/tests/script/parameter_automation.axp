      </params>
      <attribs/>
   </obj>
   <obj type="ctrl/dial p" sha="1f21216639bb798a4ea7902940999a5bcfd0de90" name="c2" x="98" y="140">
      <params>
         <frac32.u.map name="value" MidiCC="4" value="31.0"/>
