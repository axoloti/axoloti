<objdefs>
   <obj.normal id="phasor compl" uuid="bdd623d618e38f2f40bc68b36346239756548637" sha="bb083644af50d7543fc2c79e9f18d3278d964f81">
      <upgradeSha>bb3797f6808c1fa0ccf714c27361878a3fd8be29</upgradeSha>
      <sDescription>FM phasor, linear interpolated table, s-rate pitch input, 180 degree output</sDescription>
      <author>Johannes Taelman</author>
      <license>BSD</license>
      <helpPatch>osc.axh</helpPatch>
      <inlets>
         <frac32 name="pitchm" description="phase increment"/>
         <frac32buffer name="fm" description="phase increment"/>
      </inlets>
      <outlets>
         <frac32buffer.positive name="phasor0" description="phasor wave"/>
         <frac32buffer.positive name="phasor180" description="phasor wave, 180 degrees shifted"/>
      </outlets>
      <displays/>
      <params>
         <frac32.s.map.pitch name="pitch" noLabel="true"/>
      </params>
      <attribs/>
      <code.declaration><![CDATA[uint32_t Phase;]]></code.declaration>
      <code.init><![CDATA[Phase = 0;]]></code.init>
      <code.krate><![CDATA[   uint32_t freq;
   MTOFEXTENDED(param_pitch + inlet_pitchm,freq);
]]></code.krate>
      <code.srate><![CDATA[Phase += (freq>>0) + inlet_fm;
   outlet_phasor0 = Phase>>5;
   outlet_phasor180 = (Phase+(1<<31))>>5;
]]></code.srate>
   </obj.normal>
</objdefs>